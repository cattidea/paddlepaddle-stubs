from __future__ import annotations

from typing import Any, Optional

class TunableVariable:
    name: Any = ...
    def __init__(self, name: Any, default: Any | None = ...) -> None: ...
    @property
    def default(self): ...
    def get_state(self): ...
    @classmethod
    def from_state(cls, state: Any): ...

class Fixed(TunableVariable):
    name: Any = ...
    def __init__(self, name: Any, default: Any) -> None: ...
    def random(self, seed: Any | None = ...): ...

class Boolean(TunableVariable):
    def __init__(self, name: Any, default: bool = ...) -> None: ...
    def random(self, seed: Any | None = ...): ...

class Choice(TunableVariable):
    values: Any = ...
    def __init__(self, name: Any, values: Any, default: Any | None = ...) -> None: ...
    @property
    def default(self): ...
    def random(self, seed: Any | None = ...): ...
    def get_state(self): ...

class IntRange(TunableVariable):
    start: Any = ...
    stop: Any = ...
    step: Any = ...
    endpoint: Any = ...
    def __init__(
        self, name: Any, start: Any, stop: Any, step: int = ..., default: Any | None = ..., endpoint: bool = ...
    ) -> None: ...
    @property
    def default(self): ...
    def random(self, seed: Any | None = ...): ...
    def get_state(self): ...

class FloatRange(TunableVariable):
    stop: Any = ...
    start: Any = ...
    step: Any = ...
    endpoint: Any = ...
    def __init__(
        self,
        name: Any,
        start: Any,
        stop: Any,
        step: Any | None = ...,
        default: Any | None = ...,
        endpoint: bool = ...,
    ) -> None: ...
    @property
    def default(self): ...
    def random(self, seed: Any | None = ...): ...
    def get_state(self): ...
